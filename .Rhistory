#ESCRIBA LA FUNCION AQUI
install.packages("tidyr")
library(tidyr)
#definir funcion
#a=data frame, b y c=dos primeras columnas , d y e=nombres de las variables
long_df <- function(a,b,c,d,e) {
y=a %>% pivot_longer(!c(b,c),names_to =d, values_to = e)
return(y)
}
